<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context=".MainActivity" >

    <Button
        android:id="@+id/parsedataosm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/parse_data_osm"
        android:onClick="parseDataOSM" />
    <Button
        android:id="@+id/parsedataxml"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/parse_data_xml"
        android:onClick="parseDataXML" />
    <Button
        android:id="@+id/writedatabasebinary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/write_database_binary"
        android:onClick="writeDatabaseBinary" />
    <Button
        android:id="@+id/writedatabasexml"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/write_database_xml"
        android:onClick="writeDatabaseXML" />
    
    <TextView
        android:id="@+id/outputpane"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scrollbars="vertical" />

</LinearLayout>
